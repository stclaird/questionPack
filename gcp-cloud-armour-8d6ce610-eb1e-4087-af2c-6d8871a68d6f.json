[{"questionText":"Which of the following are key components of Cloud Armor's security policies?","type":"","category":"Cloud Armor","subcategory":"Security Policies","dateadded":"12-November-2025","certification":"","answerReference":"Cloud Armor Documentation","answers":[{"text":"Rules","iscorrect":true},{"text":"Policies","iscorrect":true},{"text":"Firewall Groups","iscorrect":false},{"text":"Security Hubs","iscorrect":false},{"text":"Network ACLs","iscorrect":false}]},{"questionText":"What types of traffic can Cloud Armor protect?","type":"","category":"Cloud Armor","subcategory":"Protection Capabilities","dateadded":"12-November-2025","certification":"","answerReference":"Cloud Armor Documentation","answers":[{"text":"HTTP/HTTPS traffic","iscorrect":true},{"text":"TCP traffic on non-standard ports","iscorrect":true},{"text":"UDP traffic","iscorrect":false},{"text":"SSH traffic","iscorrect":false},{"text":"ICMP traffic","iscorrect":false}]},{"questionText":"Which of the following are common use cases for Cloud Armor?","type":"","category":"Cloud Armor","subcategory":"Use Cases","dateadded":"12-November-2025","certification":"","answerReference":"Cloud Armor Documentation","answers":[{"text":"DDoS attack mitigation","iscorrect":true},{"text":"Web Application Firewall (WAF) protection","iscorrect":true},{"text":"Serverless function deployment","iscorrect":false},{"text":"Database replication","iscorrect":false},{"text":"Container orchestration","iscorrect":false}]},{"questionText":"What action can Cloud Armor rules take when a condition is met?","type":"","category":"Cloud Armor","subcategory":"Rules Actions","dateadded":"12-November-2025","certification":"","answerReference":"Cloud Armor Documentation","answers":[{"text":"Allow","iscorrect":true},{"text":"Deny","iscorrect":true},{"text":"Redirect","iscorrect":false},{"text":"Update","iscorrect":false},{"text":"Patch","iscorrect":false}]},{"questionText":"Which match conditions can be used in Cloud Armor rules?","t